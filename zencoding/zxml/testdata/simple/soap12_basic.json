{
  "env:Envelope": {
    "@xmlns:env": "http://www.w3.org/2003/05/soap-envelope",
    "@xmlns:xsd": "http://www.w3.org/2001/XMLSchema",
    "@xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
    "env:Body": {
      "m:GetUserInfo": {
        "@xmlns:m": "http://example.com/user",
        "m:UserId": { "$": "12345" }
      }
    },
    "env:Header": {
      "auth:Authentication": {
        "@xmlns:auth": "http://example.com/auth",
        "auth:Password": { "$": "pass" },
        "auth:Username": { "$": "user" }
      }
    }
  }
}
